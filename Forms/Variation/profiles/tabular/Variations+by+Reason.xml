<?xml version="1.0" encoding="utf-8"?>
<TabularReportProfile xmlns="http://ebms.com.au/schema/interchange/1.1">
  <Columns>
    <Column Name="Contract Number" Alias="" Aggregate="None" Width="148" />
    <Column Name="Reason for Variation" Alias="" Aggregate="None" Width="133" />
    <Column Name="Date Submitted" Alias="" Aggregate="None" Width="140" />
    <Column Name="Date Approved" Alias="" Aggregate="None" Width="128" />
    <Column Name="Variation to Date" Alias="Vary Date?" Aggregate="None" Width="100" />
    <Column Name="Variation to Price" Alias="Vary Value?" Aggregate="None" Width="103" />
    <Column Name="Variation Value" Alias="" Aggregate="Sum" Width="159" />
    <Column Name="New Contract Value" Alias="New Contract Value" Aggregate="Sum" Width="159" />
    <Column Name="Status" Alias="" Aggregate="None" Width="136" />
  </Columns>
  <BackgroundColourColumn xsi:nil="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
  <BackgroundColourRules />
  <TextColourColumn xsi:nil="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
  <TextColourRules />
  <AutomaticUpdates>0</AutomaticUpdates>
  <Filter>
    <GroupingExpression Operator="And">
      <Children />
    </GroupingExpression>
  </Filter>
  <ShowOnlyMyEForms>false</ShowOnlyMyEForms>
  <Status xsi:nil="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
  <StatusNotClosed>false</StatusNotClosed>
  <ShowOnlyFirstLine>true</ShowOnlyFirstLine>
  <Priority xsi:nil="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" />
  <OrderBy>[RecordID] DESC, [Reason for Variation] DESC</OrderBy>
  <GroupBy>[Reason for Variation] DESC</GroupBy>
  <GroupsStartCollapsed>true</GroupsStartCollapsed>
  <DefaultPageSize>20</DefaultPageSize>
  <PdfSettings>
    <PageLayout>A4Landscape</PageLayout>
    <Scale>1</Scale>
  </PdfSettings>
  <ExportViewLinks>false</ExportViewLinks>
  <OpenPopup>false</OpenPopup>
  <Security>
    <Roles>
      <RoleEntry Read="true" Administrate="true">
        <Role>Administrators</Role>
      </RoleEntry>
      <RoleEntry Read="true" Administrate="false">
        <Role>Everyone</Role>
      </RoleEntry>
    </Roles>
  </Security>
</TabularReportProfile>